<h1>Questionnaires</h1>

<%= link_to new_questionnaire_path do %>
  create questionnaire
<% end %>

<section>
<% @questionnaires.each do |questionnaire| %>
  <%= link_to questionnaire_path(questionnaire) do %>
    <div class="card">
      <header><%= questionnaire.title %></header>
    </div>
  <% end %>
<% end %>
</section>
